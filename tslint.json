{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "jsx-boolean-value": ["error", "never"],
    "jsx-no-multiline-js": ["error", "never"],
    "max-line-length": [
      true,
      120
    ],
    "eofline": true,
    "curly": true,
    "import-spacing": true,
    "indent": [
      true,
      "spaces",
      2
    ]
  }
}